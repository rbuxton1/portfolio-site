  <script type="text/javascript">
    var imgs=["bench.jpg", "colors.jpg", "manitou.jpg", "river.jpg", "sammie.jpg", "street.jpg",
              "trail.jpg", "two_idiots.jpg", "undergrowth1.jpg", "undergrowth2.jpg"];
    var pos = 1;

    function fadeIn(element) {
      var op = 0.1;  // initial opacity
      var timer = setInterval(function () {
        if (op >= 1){
          clearInterval(timer);
        }
        element.style.opacity = op;
        op += 0.1;
      }, 50);
    }
    function fadeOutAndCallBack(image, callback){
      var opacity = 1;
      var timer = setInterval(function(){
        if(opacity < 0.1){
          clearInterval(timer);
          callback();
        }
        image.style.opacity = opacity
        opacity -= .1;
      }, 50);
    }

    function gallery(){
      fadeOutAndCallBack(document.getElementById("titleImage"), function(){
        document.getElementById("titleImage").src  = imgs[pos];
        if(pos < imgs.length - 1) pos++;
        else pos = 0;
        fadeIn(document.getElementById("titleImage"))
      })
    }

    window.onload = setInterval(gallery, 5000);
  </script>

